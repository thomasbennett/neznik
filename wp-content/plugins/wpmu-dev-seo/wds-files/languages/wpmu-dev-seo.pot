"\t</tfoot>\n"
"\t<tbody>\n"
"\t\t<tr>\n"
"\t\t\t<th>%%date%%</th>\n"
"\t\t\t<td>Replaced with the date of the post/page</td>\n"
"\t\t</tr>\n"
"\t\t<tr class=\"alt\">\n"
